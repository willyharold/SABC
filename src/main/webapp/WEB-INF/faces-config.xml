<?xml version='1.0' encoding='UTF-8'?>

<!-- =========== FULL CONFIGURATION FILE ================================== -->

<faces-config version="2.1"
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_1.xsd">

    <application>
        <el-resolver>
            org.springframework.web.jsf.el.SpringBeanFacesELResolver
        </el-resolver>
    </application>
    <navigation-rule>
        <from-view-id>/index.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>connexion</from-outcome>
            <to-view-id>/equipement.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>erreur</from-outcome>
            <to-view-id>/index.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    
    <navigation-rule>
        <from-view-id>/*</from-view-id>
        <navigation-case>
            <from-outcome>liste_agences</from-outcome>
            <to-view-id>/agence/liste_agences.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    
    <navigation-rule>
        <from-view-id>/*</from-view-id>
        <navigation-case>
            <from-outcome>liste_comptes</from-outcome>
            <to-view-id>/compte/liste_comptes.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/*</from-view-id>
        <navigation-case>
            <from-outcome>liste_imprimantes</from-outcome>
            <to-view-id>/imprimante/liste_imprimantes.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>consommable</from-outcome>
            <to-view-id>/imprimante/commander.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/*</from-view-id>
        <navigation-case>
            <from-outcome>liste_services</from-outcome>
            <to-view-id>/service/liste_services.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/*</from-view-id>
        <navigation-case>
            <from-outcome>liste_categories</from-outcome>
            <to-view-id>/categorie/liste_categories.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/*</from-view-id>
        <navigation-case>
            <from-outcome>liste_personnels</from-outcome>
            <to-view-id>/personne/liste_personnels.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/*</from-view-id>
        <navigation-case>
            <from-outcome>liste_comptes</from-outcome>
            <to-view-id>/compte/liste_comptes.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/*</from-view-id>
        <navigation-case>
            <from-outcome>deconnexion</from-outcome>
            <to-view-id>/index.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    
    <navigation-rule>
        <from-view-id>/*</from-view-id>
        <navigation-case>
            <from-outcome>messagerie</from-outcome>
            <to-view-id>/messagerie/consulter.xhtml</to-view-id>
        </navigation-case>
        
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/*</from-view-id>
        <navigation-case>
            <from-outcome>imprimant</from-outcome>
            <to-view-id>/menu.xhtml</to-view-id>
        </navigation-case>
        
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/*</from-view-id>
        <navigation-case>
            <from-outcome>encre</from-outcome>
            <to-view-id>/imprimante/liste_encres.xhtml</to-view-id>
        </navigation-case>
        
    </navigation-rule>
    
    <navigation-rule>
        <from-view-id>/*</from-view-id>
        <navigation-case>
            <from-outcome>papier</from-outcome>
            <to-view-id>/imprimante/liste_papier.xhtml</to-view-id>
        </navigation-case>
        
    </navigation-rule>
    
    <navigation-rule>
        <from-view-id>/*</from-view-id>
        <navigation-case>
            <from-outcome>total</from-outcome>
            <to-view-id>/statistique/Total.xhtml</to-view-id>
        </navigation-case>
        
    </navigation-rule>
    
    <navigation-rule>
        <from-view-id>/*</from-view-id>
        <navigation-case>
            <from-outcome>mensuelle</from-outcome>
            <to-view-id>/statistique/mensuelle.xhtml</to-view-id>
        </navigation-case>
        
    </navigation-rule>
    
    <navigation-rule>
        <from-view-id>/*</from-view-id>
        <navigation-case>
            <from-outcome>annuelle</from-outcome>
            <to-view-id>/statistique/annuelle.xhtml</to-view-id>
        </navigation-case>
        
    </navigation-rule>
</faces-config>
